<h1> Enter A New Product </h1>


<%= form_for @product do |f| %>
  <%= f.label :name, "Product Name" %>
  <%= f.text_field :name %>
  <%= f.label :country, "Country of Origin" %>
  <%= f.text_field :country %>
  <%= f.label :cost, "Product Cost" %>
  <%= f.text_field :cost %>
  <%= f.submit %>
<% end %> 
