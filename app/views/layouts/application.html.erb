<!DOCTYPE html>
<html>
  <head>
    <title>MariosSpecialityFoods</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>


  <body>
    <nav class="navbar  navbar-inverse bg-primary">
      <nav class="navbar-brand">
        <%= link_to root_path do %>
          <%= image_tag "mario.png", class: "thumbnail2" %>
        <% end %>
      </nav>
      <nav>
        <p class="navbar-brand"><%= link_to "Products", products_path %></p>
      </nav>
    </nav>

    <div class="container">
      
      <%= flash[:alert] %>
      <%= flash[:notice] %>
      <%= yield %>

    </div>
  </body>
</html>
